{
  "name": "catenary-frontend",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "cloudflarebuild": "mv cloudflare-svelte.config.js svelte.config.js && vite build",
    "preview": "vite preview --port 16172",
    "betapreview": "vite preview --port 16173",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-node": "^5.3.3",
    "@sveltejs/kit": "^2.46.5",
    "@types/mapbox__polyline": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@wasm-tool/rollup-plugin-rust": "^3.0.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "rollup-plugin-svelte": "^7.2.3",
    "svelte": "^5.39.12",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.1.14",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vitest": "^4.0.1"
  },
  "type": "module",
  "dependencies": {
    "@cheprasov/json-web-worker": "^1.0.0",
    "@mapbox/polyline": "^1.2.1",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/vite": "^4.1.14",
    "@turf/turf": "^7.2.0",
    "@zerodevx/svelte-toast": "^0.9.6",
    "base65536": "^4.0.3",
    "geolocation": "^0.2.0",
    "gtfs-realtime-bindings": "^1.1.1",
    "haversine-distance": "^1.2.4",
    "maplibre-contour": "^0.1.0",
    "maplibre-gl": "^5.9.0",
    "moment": "^2.30.1",
    "protobufjs": "^7.5.4",
    "store2": "^2.14.4",
    "stringify-object": "^5.0.0",
    "svelte-i18n": "^4.0.1",
    "svelte-toasts": "^1.1.2",
    "tailwind": "^4.0.0",
    "toastify-js": "^1.12.0",
    "wrangler": "^4.44.0"
  }
}
