		"moment": "^2.30.1",
		"mta-subway-bullets": "^1.0.0",
		"protobufjs": "^7.5.4",
		"store2": "^2.14.4",
		"stringify-object": "^5.0.0",
		"svelte-i18n": "^4.0.1",
		"svelte-toasts": "^1.1.2",
		"tailwind": "^4.0.0",
		"toastify-js": "^1.12.0",
