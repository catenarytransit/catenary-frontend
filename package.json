{
  "name": "catenary-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "cloudflarebuild": "mv cloudflare-svelte.config.js svelte.config.js && vite build",
    "preview": "vite preview --port 16172",
    "betapreview": "vite preview --port 16173",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.17.1",
    "@types/mapbox__polyline": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@wasm-tool/rollup-plugin-rust": "^3.0.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "^8.5.1",
    "prettier": "^3.5.0",
    "prettier-plugin-svelte": "^3.3.3",
    "rollup-plugin-svelte": "^7.2.2",
    "svelte": "^5.20.2",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^4.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.2.4",
    "vitest": "^3.0.5"
  },
  "type": "module",
  "dependencies": {
    "@jill64/sentry-sveltekit-cloudflare": "^2.0.6",
    "@mapbox/polyline": "^1.2.1",
    "@sentry/sveltekit": "^8.54.0",
    "@sentry/vite-plugin": "^3.2.3",
    "@sveltejs/adapter-cloudflare": "^5.0.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/postcss": "^4.0.5",
    "@tailwindcss/vite": "^4.0.17",
    "@turf/turf": "^7.2.0",
    "@zerodevx/svelte-toast": "^0.9.6",
    "base65536": "^4.0.3",
    "geolocation": "^0.2.0",
    "gtfs-realtime-bindings": "^1.1.1",
    "maplibre-contour": "^0.1.0",
    "maplibre-gl": "^5.1.0",
    "moment": "^2.30.1",
    "protobufjs": "^7.4.0",
    "store2": "^2.14.4",
    "stringify-object": "^5.0.0",
    "svelte-i18n": "^4.0.1",
    "svelte-toasts": "^1.1.2",
    "tailwind": "^4.0.0",
    "toastify-js": "^1.12.0",
    "wrangler": "^3.107.3"
  }
