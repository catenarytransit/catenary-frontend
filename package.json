	"type": "module",
	"dependencies": {
		"@cheprasov/json-web-worker": "^1.0.0",
		"@mapbox/polyline": "^1.2.1",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.14",
		"@tailwindcss/vite": "^4.1.14",
		"@turf/turf": "^7.2.0",
